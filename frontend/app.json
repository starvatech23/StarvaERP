{
  "expo": {
    "name": "SiteOps",
    "slug": "siteops",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "siteops",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.starva.siteops"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F97316"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.starva.siteops"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "SiteOps - Site Management",
      "shortName": "SiteOps",
      "description": "India's Complete Site Management Application by Starva",
      "backgroundColor": "#FFFFFF",
      "themeColor": "#F97316"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F97316"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
