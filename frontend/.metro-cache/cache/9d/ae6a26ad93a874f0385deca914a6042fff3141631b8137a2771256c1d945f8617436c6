    return '#EF4444';\n        default:\n          return '#6B7280';\n      }\n    };\n    const formatDate = dateString => {\n      const date = new Date(dateString);\n      return date.toLocaleDateString('en-IN', {\n        day: 'numeric',\n        month: 'short',\n        year: 'numeric',\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    };\n    const renderUpdatesView = () => /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(ScrollView.default, {\n      style: styles.content,\n      children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n        style: styles.filterRow,\n        children: ['all', 'daily', 'weekly', 'monthly'].map(freq => /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(TouchableOpacity.default, {\n          style: [styles.filterChip, frequencyFilter === freq && styles.filterChipActive],\n          onPress: () => setFrequencyFilter(freq),\n          children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: [styles.filterChipText, frequencyFilter === freq && styles.filterChipTextActive],\n            children: freq.charAt(0).toUpperCase() + freq.slice(1)\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 658,\n            columnNumber: 13\n          }, this)\n        }, freq, false, {\n          fileName: _jsxFileName,\n          lineNumber: 653,\n          columnNumber: 11\n        }, this))\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 651,\n        columnNumber: 7\n      }, this), updates.length === 0 ? /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n        style: styles.emptyState,\n        children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoVectorIcons.Ionicons, {\n          name: \"document-text-outline\",\n          size: 64,\n          color: \"#CBD5E0\"\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 667,\n          columnNumber: 11\n        