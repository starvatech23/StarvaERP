canOpen = await Linking.default.canOpenURL(whatsappUrl);\n        if (canOpen) {\n          await Linking.default.openURL(whatsappUrl);\n        } else {\n          // Fallback to web WhatsApp\n          const webUrl = `https://wa.me/${formattedPhone}?text=${encodeURIComponent(message)}`;\n          await Linking.default.openURL(webUrl);\n        }\n      } catch (error) {\n        Alert.default.alert('Error', 'Unable to open WhatsApp. Please make sure WhatsApp is installed.');\n      }\n    };\n\n    // Build shareable message for status update\n    const buildShareMessage = update => {\n      const projectName = project?.name || 'Project';\n      const lines = [`üìä *Project Status Update*`, `‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ`, `üèóÔ∏è *Project:* ${projectName}`, `üìù *Update:* ${update.title}`, ``];\n      if (update.description) {\n        lines.push(`üìã *Details:*`);\n        lines.push(update.description);\n        lines.push(``);\n      }\n      lines.push(`üìà *Progress Summary:*`);\n      lines.push(`   ‚úÖ Completed: ${update.tasks_completed} tasks`);\n      lines.push(`   üîÑ In Progress: ${update.tasks_in_progress} tasks`);\n      lines.push(`   ‚è≥ Pending: ${update.tasks_pending} tasks`);\n      lines.push(`   üìä Overall: ${update.overall_progress}%`);\n      lines.push(``);\n      if (update.weather) {\n        lines.push(`üå§Ô∏è *Weather:* ${update.weather}`);\n      }\n      if (update.issues) {\n        lines.push(`‚ö†Ô∏è *Issues/Blockers:*`);\n        lines.push(update.issues);\n        lines.push(``);\n      }\n      if (update.next_steps) {\n        lines.push(`‚û°Ô∏è *Next Steps:*`);\n        lines.push(update.next_steps);\n        lines.push(``);\n      }\n      lines.push(`‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ`);\n      lines.push(`üìÖ ${formatDate(update.created_at)}`);\n      lines.push(`üë§ By: ${update.created_by_name}`);\n      return lines.join('\\n');\n    };\n\n    // Generate HTML for PDF with status u