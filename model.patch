-----------------------------+    if not conversation:
-----------------------------+        raise HTTPException(status_code=404, detail="Conversation not found")
-----------------------------+    
-----------------------------+    if current_user["_id"] not in conversation["participants"]:
-----------------------------+        raise HTTPException(status_code=403, detail="Not a participant in this conversation")
-----------------------------+    
