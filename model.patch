---------------------------------------------------+                "full_name": f"Test Project Manager {timestamp}",
---------------------------------------------------+                "role": role,
---------------------------------------------------+                "phone": f"+91987654{(timestamp + random_suffix) % 10000}",
---------------------------------------------------+                "auth_type": "email"
---------------------------------------------------+            }
---------------------------------------------------+            
---------------------------------------------------+            response = requests.post(f"{self.base_url}/auth/register", json=user_data)
